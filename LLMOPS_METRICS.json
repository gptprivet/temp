          "refId": "A"
        },
        {
          "datasource": { "type": "grafana-clickhouse-datasource", "uid": "${datasource_clickhouse}" },
          "query": "SELECT toStartOfInterval(timestamp, INTERVAL 5 MINUTE) AS time, quantile(0.95)(latency) AS value FROM langfuse.traces WHERE timestamp >= $__from AND timestamp <= $__to AND latency IS NOT NULL GROUP BY time ORDER BY time",
          "rawQuery": true,
          "legendFormat": "Langfuse trace p95",
          "refId": "B"
        }
      ],
      "title": "Latency p95",
      "type": "timeseries"
