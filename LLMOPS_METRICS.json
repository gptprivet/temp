          "refId": "A"
        },
        {
          "datasource": { "type": "grafana-clickhouse-datasource", "uid": "${datasource_clickhouse}" },
          "query": "SELECT toStartOfInterval(timestamp, INTERVAL 5 MINUTE) AS time, countIf(level = 'ERROR') / nullIf(count(), 0) * 100 AS value FROM langfuse.traces WHERE timestamp >= $__from AND timestamp <= $__to GROUP BY time ORDER BY time",
          "rawQuery": true,
          "legendFormat": "Langfuse traces",
          "refId": "B"
        }
      ],
      "title": "Error Rate",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource_prometheus}" },
