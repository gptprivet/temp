        {
          "datasource": { "type": "grafana-clickhouse-datasource", "uid": "${datasource_clickhouse}" },
          "query": "SELECT toStartOfInterval(timestamp, INTERVAL 5 MINUTE) AS time, count() / 300.0 AS value FROM langfuse.traces WHERE timestamp >= $__from AND timestamp <= $__to GROUP BY time ORDER BY time",
          "rawQuery": true,
          "legendFormat": "Langfuse traces",
          "refId": "B"
        }
      ],
