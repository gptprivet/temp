          "datasource": { "type": "prometheus", "uid": "${datasource_prometheus}" },
          "expr": "sum(rate(qdrant_grpc_server_requests_total[5m])) or vector(0)",
          "legendFormat": "Requests",
          "refId": "A"
        }
      ],
      "title": "Qdrant: gRPC requests",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource_prometheus}" },
