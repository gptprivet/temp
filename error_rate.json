        }
      },
      "gridPos": { "h": 6, "w": 8, "x": 8, "y": 15 },
      "id": 31,
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${datasource_prometheus}" },
          "expr": "sum(rate(vllm_num_tokens_generated_total[5m])) or vector(0)",
          "legendFormat": "tokens/sec",
          "refId": "A"
        }
      ],
