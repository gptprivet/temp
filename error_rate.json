      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${datasource_prometheus}" },
          "expr": "sum(rate(qdrant_grpc_server_handled_total[5m])) by (grpc_code) or vector(0)",
          "legendFormat": "{{grpc_code}}",
          "refId": "A"
